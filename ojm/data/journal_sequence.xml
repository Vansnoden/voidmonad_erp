<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data noupdate="1">
        <record id="sequence_journal" model="ir.sequence">
            <field name="name">Journal Sequence</field>
            <field name="code">ojm.journal.sequence</field>
            <field name="prefix">J-</field>
            <field name="padding">5</field>
            <field name="number_next_actual">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"></field>
        </record>

        <record id="sequence_journal_issue" model="ir.sequence">
            <field name="name">Journal Issue Sequence</field>
            <field name="code">ojm.journal.issue.sequence</field>
            <field name="prefix">JI-</field>
            <field name="padding">5</field>
            <field name="number_next_actual">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"></field>
        </record>

        <record id="sequence_journal_volume" model="ir.sequence">
            <field name="name">Journal Volume Sequence</field>
            <field name="code">ojm.journal.volume.sequence</field>
            <field name="prefix">JV-</field>
            <field name="padding">5</field>
            <field name="number_next_actual">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"></field>
        </record>
    </data>
</odoo>